{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "version": 2,
  "final_space": true,
  "console_title_template": "{{ .Folder }}",
  "blocks": [
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "type": "session",
          "style": "diamond",
          "foreground": "#000",
          "background": "#16c60c",
          "leading_diamond": "\ue0b6",
          "trailing_diamond": "\ue0b0",
          "template": " {{ if .SSHSession }}\uf817 {{ end }}{{ .UserName }} "
        },
        {
          "type": "path",
          "style": "powerline",
          "powerline_symbol": "\ue0b0",
          "foreground": "#000000",
          "background": "#32afff",
          "template": " \uf07b  {{ .Path }} ",
          "properties": {
            "style": "folder"
          }
        },
        {
          "type": "git",
          "style": "powerline",
          "powerline_symbol": "\ue0b0",
          "foreground": "#ffffff",
          "background": "#ad7fa8",

          "background_templates": [
            "{{ if or (.Working.Changed) (.Staging.Changed) }} #de7b42 {{ end }}",
            "{{ if and (gt .Ahead 0) (gt .Behind 0) }} #ff4500 {{ end }}",
            "{{ if gt .Ahead 0 }} #2e86ff {{ end }}",
            "{{ if gt .Behind 0 }} #c52424 {{ end }}"
          ],
          "template": " {{ .UpstreamIcon }} {{ .HEAD }}{{if .BranchStatus }} {{ .BranchStatus }}{{ end }}{{ if .Working.Changed }} \uf044 {{ .Working.String }}{{ end }}{{ if and (.Working.Changed) (.Staging.Changed) }} |{{ end }}{{ if .Staging.Changed }} \uf046 {{ .Staging.String }}{{ end }}{{ if gt .StashCount 0 }} \uf692 {{ .StashCount }}{{ end }} ",
          "properties": {
            "fetch_stash_count": true,
            "fetch_status": true,
            "fetch_upstream_icon": true
          }
        },
        {
          "type": "exit",
          "style": "diamond",
          "foreground": "#ffffff",
          "background": "#6CA35E",
          "background_templates": ["{{ if gt .Code 0 }}#f1184c{{ end }}"],
          "leading_diamond": "<transparent,background>\ue0b0</>",
          "trailing_diamond": "\ue0b4",
          "template": " {{ if gt .Code 0 }}\uf00d {{ .Meaning }}{{ else }}\uf42e{{ end }} ",
          "properties": {
            "always_enabled": true
          }
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "right",
      "segments": [
        {
          "type": "executiontime",
          "style": "plain",
          "foreground": "#777e9d",
          "background": "#404456",
          "template": "<#404456,transparent>\ue0b6</>祥 {{ .FormattedMs }}\u202f<transparent,#404456>\ue0b2</>",
          "properties": {
            "always_enabled": true,
            "style": "austin"
          }
        },
        {
          "type": "nx",
          "style": "powerline",
          "powerline_symbol": "\ue0b2",
          "invert_powerline": true,
          "foreground": "#ffffff",
          "background": "#1976d2",
          "template": " \ue753 {{ .Major }} "
        },
        {
          "type": "angular",
          "style": "powerline",
          "powerline_symbol": "\ue0b2",
          "invert_powerline": true,
          "foreground": "#ffffff",
          "background": "#c4002b",
          "template": " \ue753 {{ .Major }} "
        },
        {
          "type": "node",
          "style": "powerline",
          "powerline_symbol": "\ue0b2",
          "invert_powerline": true,
          "foreground": "#ffffff",
          "background": "#6CA35E",
          "template": " \ue718 {{.Major}} "
        },
        {
          "type": "npm",
          "style": "powerline",
          "powerline_symbol": "\ue0b2",
          "invert_powerline": true,
          "foreground": "#ffffff",
          "background": "#d3504f",
          "template": " \ue71e {{ .Major }} "
        },
        // {
        //   "type": "battery",
        //   "style": "powerline",
        //   "powerline_symbol": "\ue0b2",
        //   "invert_powerline": true,
        //   "foreground": "#ffffff",
