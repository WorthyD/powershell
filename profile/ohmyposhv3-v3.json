        //   "foreground": "#ffffff",
        //   "background": "#1976d2",
        //   "template": "{{.Weather}} ({{.Temperature}}{{.UnitIcon}})",
        //   "properties": {
        //     "api_key": "",
        //     "location": "Cave Springs,AR,US",
        //     "units": "imperial",
